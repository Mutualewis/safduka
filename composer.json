{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "niklasravnsborg/laravel-pdf": "^1.1",
        "mgallegos/laravel-jqgrid": "dev-master",
        "nayjest/grids": "^1.3",
        "laravelcollective/html": "^5.2",
        "spatie/activitylog": "^2.4",
        "zizaco/entrust": "5.2.x-dev",
        "maatwebsite/excel": "~2.1.0",
        "yajra/laravel-datatables-oracle": "6.x",
        "jimmyjs/laravel-report-generator": "^1.0",
        "h4cc/wkhtmltopdf-i386": "0.12.x",
        "h4cc/wkhtmltoimage-i386": "0.12.x",
        "knplabs/knp-snappy": "^0.5.0",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "barryvdh/laravel-snappy": "^0.3.3",
        "reportico/laravel-reportico": "~5.2",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3",
        "view-components/view-components": "^0.26.6",
        "doctrine/dbal": "^2.5",
        "league/fractal": "^0.17.0",
        "zofe/rapyd": "2.2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Ngea\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
